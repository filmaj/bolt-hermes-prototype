{
    "manifest": {
        "command": {
            "default": "npm run manifest --silent"
        }
    },
    "triggers": {
        "command": {
            "default": "node -e 'console.log(\"[]\")'"
        }
    },
    "run": {
        "command": {
            "default": "node index.js"
        },
        "watcher": {
            "filter_regex": "^.*\\.js$",
            "paths": [
                "./"
            ]
        }
    }
}
